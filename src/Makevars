	libv8/src/base/platform/platform-posix.o

all: libv8_base.a libv8_libplatform.a libicui18n.a libicuuc.a libv8_libbase.a

clean:
	rm -f libv8_base.a libv8_libplatform.a libicui18n.a libicuuc.a libv8_libbase.a
	rm -f $(V8BASE) $(LIBPLATFORM) $(ICUI18N) $(ICUUC) $(LIBBASE)

libv8_base.a: $(V8BASE)
	$(AR) rcs libv8_base.a $(V8BASE)

libv8_libplatform.a: $(LIBPLATFORM)
	$(AR) rcs libv8_libplatform.a $(LIBPLATFORM)

libicui18n.a: $(ICUI18N)
	$(AR) rcs libicui18n.a $(ICUI18N)

libicuuc.a: $(ICUUC)
	$(AR) rcs libicuuc.a $(ICUUC)

libv8_libbase.a: $(LIBBASE)
	$(AR) rcs libv8_libbase.a $(LIBBASE)
